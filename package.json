{
  "name": "rel-vdoms",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "ava": {
    "babel": "inherit",
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "clean": "rimraf .fusebox src/gen/*.js",
    "prepare": "yarn run clean; yarn build:master",
    "build:master": "node script/gen-code.js",
    "watch:master": "chokidar masterdata/**/* -c 'yarn build:master' --polling",
    "build:src": "node fuse-build.js",
    "watch:src": "WATCH=1 node fuse-build.js",
    "watch": "yarn prepare; run-p watch:*",
    "test": "ava"
  },
  "devDependencies": {
    "@mizchi/babel-preset": "^1.1.1",
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.19.0",
    "eslint-config-mizchi": "^1.0.0",
    "eslint-plugin-mutation": "^1.0.0",
    "flow-bin": "^0.45.0",
    "fuse-box": "^1.3.131",
    "json-schema-to-flow-type": "^0.2.6",
    "npm-run-all": "^4.0.2",
    "require-yaml": "^0.0.1",
    "rimraf": "^2.6.1",
    "tv4": "^1.3.0"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "lodash.range": "^3.2.0",
    "prop-types": "^15.5.8",
    "react": "next",
    "react-dom": "next",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.15.0",
    "redux-thunk": "^2.2.0",
    "seamless-immutable": "^7.1.2"
  }
}
